<p id="notice"><%= notice %></p>

<h1>Fingerprints</h1>

<table>
  <thead>
    <tr>
      <th>Track ID</th>
      <th>Matches</th>
      <th>Fingerprint</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fingerprints.each do |fingerprint| %>
      <tr>
        <td><%= fingerprint.track_id %></td>
        <td><%= %></td>
        <td><%= truncate(fingerprint.fingerprint, length: 67, separator: ' ') %></td>
        <td><%= link_to 'Show', fingerprint %></td>
        <td><%= link_to 'Edit', edit_fingerprint_path(fingerprint) %></td>
        <td><%= link_to 'Destroy', fingerprint, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fingerprint', new_fingerprint_path %>

<h2><%= link_to 'Home', root_path %></h2>
